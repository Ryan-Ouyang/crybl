            linearGradient#grad1(x1="0%" y1="0%" x2="100%" y2="0%")
              stop(offset="0%" style="stop-color: #3B95F3" stop-opacity="0.5")
              stop(offset="100%" style="stop-color: #6ECDEE" stop-opacity="1")
          rect(width="200px" height="80px" rx="10" ry="10" fill="url(#grad1)")

        svg#smallRectangle2(width="200px" height="80px")
          defs
            linearGradient#grad1(x1="0%" y1="0%" x2="100%" y2="0%")
              stop(offset="0%" style="stop-color: #6ECDEE" stop-opacity="0.5")
              stop(offset="100%" style="stop-color: #3B95F3" stop-opacity="1")
          rect(width="100px" height="40px" rx="10" ry="10" fill="url(#grad1)")

          
